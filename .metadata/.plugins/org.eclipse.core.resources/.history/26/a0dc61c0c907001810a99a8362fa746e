/**
 * @author Jiang
 * rename files names in the same folder
 */
package util;

import   java.io.*;     

public   class   Rename   
{     
        public   static   void   main(String[]   args)   {     
        	
          File directory = new File("G:/计算机学习/ETL/【03.InformaticaETL工具视频教程】/InformaticaETL工具视频教程 30课"); 
          
          File []files=directory.listFiles();
          
          String filename;
          
          for(int i=0;i<files.length;i++){
        	  filename = files[i].toString();
        	  if(filename.contains("[www.17zixueba.com]下载更多视频教程_20171008153749")){
        		  filename = filename.substring(filename.indexOf("[www.17zixueba.com]")+19);
        		  File newFile = new File("d:/"+filename);
        		  files[i].renameTo(newFile);
        		  System.out.println("ok.");
        	  }
        	  System.out.println(filename);
          }
      }     
      
}  