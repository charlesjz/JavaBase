import td.BankingService;
import bmo.mortgage.MortgageLoan;

public class MyFirstProgram {

	public static void main ( String[] args ) {

		System.out.println ( "Hello, this is my first Java program." );

		BankingService service = new BankingService ( );
		service.describeService ( );

		cibc.BankingService service2 = new cibc.BankingService ( );
		service2.describeService ( );

		MortgageLoan mortgageLoan = new MortgageLoan ( );
		mortgageLoan.apply ( );
		
		int intNumber = 36;
		intNumber = 0x24;
		intNumber = 0b00100100;
		
		long intNumber2 = 99999999999999L;
		
		System.out.println ( "intNumber = " + intNumber );
		
		String s = "abc\t\n\r123\u0108";
		System.out.println( "s = " + s );
		
		int array1[] = { 1, 2, 3, 4, 5, 6, 5, 4, 3, 2, 1, 0 };
		int array2[] = new int[3];
		System.arraycopy(array1, 4, array2, 0, 3);
		
		System.out.println( "array1 = " + array1[0] + ", " + array1[1] + ", ..., " + array1[array1.length-1] );
		System.out.println( "array2 = " + array2[0] + ", " + array2[1] + ", " + array2[2] );
		

	}

}